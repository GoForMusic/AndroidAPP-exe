<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.androidapp_exe.ui.Home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.androidapp_exe.ui.Dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_news_item"
        android:name="com.example.androidapp_exe.ui.Home.HomeFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/layout_item" />

    <fragment
        android:id="@+id/navigation_activity_web"
        android:name="com.example.androidapp_exe.ui.Home.HomeFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/activity_web_view" >
        <action
            android:id="@+id/action_navigation_activity_web_to_navigation_home"
            app:destination="@id/navigation_home" />
    </fragment>

</navigation>